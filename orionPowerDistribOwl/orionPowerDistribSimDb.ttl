@prefix : <http://example.org/orionPowerDistribution/simDb#> .
@prefix simDb: <http://example.org/orionPowerDistribution/simDb#> .
@prefix simSchema: <http://example.org/orionPowerDistribution/simSchema#> .
@prefix modelDb: <http://example.org/orionPowerDistribution/modelDb#> .
@prefix sysML: <http://example.org/orionPowerDistribution/sysmlSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://example.org/orionPowerDistribution/simDb> .

<http://example.org/orionPowerDistribution/simDb> rdf:type owl:Ontology ;
                                                rdfs:comment "Assertions driving the simulation of the Orion Power Distribution model." ;
                                                    owl:imports <http://example.org/orionPowerDistribution/simSchema> ;
                                                    owl:imports <http://example.org/orionPowerDistribution/modelDb> .

#################################################################
#
#    Individuals
#
#################################################################


###  http://example.org/orionPowerDistribution/simSchema#AS11

:AS11 rdf:type simSchema:AttributeSet ,
               owl:NamedIndividual ;
      
      rdfs:label "Scenario 3 Converter voltage and RPC_1 current limit." ;
      
      simSchema:contains modelDb:A3 ,
                modelDb:A3261 .



###  http://example.org/orionPowerDistribution/simSchema#AS12

:AS12 rdf:type simSchema:AttributeSet ,
               owl:NamedIndividual ;
      
      rdfs:label "Scenario 3 Converter voltage and RPC_2 current limit." ;
      
      simSchema:contains modelDb:A3 ,
                modelDb:A3262 .



###  http://example.org/orionPowerDistribution/simSchema#AS13

:AS13 rdf:type simSchema:AttributeSet ,
               owl:NamedIndividual ;
      
      rdfs:label "Scenario 3 Converter voltage and RPC_3 current limit." ;
      
      simSchema:contains modelDb:A3 ,
                modelDb:A3263 .



###  http://example.org/orionPowerDistribution/simSchema#AS14

:AS14 rdf:type simSchema:AttributeSet ,
               owl:NamedIndividual ;
      
      rdfs:label "Scenario 3 Converter voltage and RPC_4 current limit." ;
      
      simSchema:contains modelDb:A3 ,
                modelDb:A3264 .



###  http://example.org/orionPowerDistribution/simSchema#AS21

:AS21 rdf:type simSchema:AttributeSet ,
               owl:NamedIndividual ;
      
      rdfs:label "Scenario 3 Load_1 resistance and current." ;
      
      simSchema:contains modelDb:A3301 ,
                modelDb:A3311 .



###  http://example.org/orionPowerDistribution/simSchema#AS22

:AS22 rdf:type simSchema:AttributeSet ,
               owl:NamedIndividual ;
      
      rdfs:label "Scenario 3 Load_2 resistance and current." ;
      
      simSchema:contains modelDb:A3302 ,
                modelDb:A3312 .



###  http://example.org/orionPowerDistribution/simSchema#AS23

:AS23 rdf:type simSchema:AttributeSet ,
               owl:NamedIndividual ;
      
      rdfs:label "Scenario 3 Load_3 resistance and current." ;
      
      simSchema:contains modelDb:A3303 ,
