###  http://example.org/orionPowerDistribution/simSchema#Comp1

:Comp1 rdf:type simSchema:Computation ,
                owl:NamedIndividual ;
       
      rdfs:label "Scenario_3 original computation of power." ;

       simSchema:hasPriority 1 ;
       
       simSchema:produces modelDb:A4 ;
       
       simSchema:uses :AS11 ,
             :AS12 ,
             :AS13 ,
             :AS14 .



###  http://example.org/orionPowerDistribution/simSchema#Comp2

:Comp2 rdf:type simSchema:Computation ,
                owl:NamedIndividual ;
       
      rdfs:label "Scenario_3 alternative computation of power." ;
      rdfs:comment "To be generated by the rule engine by compiling the constraint violation override." ;

       simSchema:hasPriority 2 ;
       
       simSchema:produces modelDb:A4 ;
       
       simSchema:uses :AS21 ,
             :AS22 ,
             :AS23 ,
             :AS24 .



